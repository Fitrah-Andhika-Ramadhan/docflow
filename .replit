run = "cd backend && pip install -r requirements.txt && uvicorn server:app --host 0.0.0.0 --port 8000"
language = "python3"
entrypoint = "backend/server.py"

[nix]
channel = "stable-22_11"

[deployment]
run = ["sh", "-c", "cd backend && uvicorn server:app --host 0.0.0.0 --port 8000"]
deploymentTarget = "cloudrun"
